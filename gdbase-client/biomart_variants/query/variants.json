{
	"dataset": { "name": "{SPECIE}_snp", "interface": "default" },
	"filters": [ { "name": "chr_name", "value": "{CHROMOSOME}" } ],
	"attributes":
	[
		{ "id": "id", "name": "refsnp_id", "type": "string" },
		{ "id": "chromosome", "name": "chr_name", "type": "string" },
